grep @A0 ClionaRe7_R2.trim.cat | wc -l
grep @A0 Unp_ClionaRe7_R1.trim.cat_ClionaRe7_R2.trim.cat | wc -l

perl /annotatingTranscriptomes/dedupTranscriptome.pl left=ClionaRe7_R1.trim.cat right=ClionaRe7_R2.trim.cat unp=Unp_ClionaRe7_R1.trim.cat_ClionaRe7_R2.trim.cat

cat ClionaRe7_R1.trim.cat.dedup | perl -pe 's/^(\@D8Q.+)$/$1\/1/' > ClionaRe7_R1p_suf1.fastq && cat Unp_ClionaRe7_R1.trim.cat_ClionaRe7_R2.trim.cat.dedup | perl -pe 's/^(\@D8Q.+)$/$1\/1/' >> ClionaRe7_R1p_suf1.fastq
cat ClionaRe7_R2.trim.cat.dedup | perl -pe 's/^(\@D8Q.+)$/$1\/2/' > ClionaRe7_R2p_suf2.fastq 






